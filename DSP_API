        - in: header
          name: sourceID
          required: true
          schema:
            type: string
        - in: header
          name: destinationID
          required: true
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: object
              properties:
                operatorId:
                  type: string
                clientIpAddress:
                  type: string
              required:
                - operatorId
                - clientIpAddress
      responses:
        '200':
          description: Successful Operation
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    code:
                      type: integer
                    nameAr:
                      type: string
                    nameEn:
                      type: string
        '400':
          description: Invalid request or missing required fields
        '401':
          description: Authentication failure
        '403':
          description: Unauthorized access attempt

